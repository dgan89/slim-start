{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "vlucas/spot2": "^2.1",
        "tuupola/cors-middleware": "0.5.2",
        "twbs/bootstrap": "4.0.0-alpha.6",
        "components/jquery": "^3.1",
        "razmik/helper": "dev-master",
        "tuupola/slim-jwt-auth": "^2.0",
        "tuupola/slim-basic-auth": "^2.0",
        "gofabian/negotiation-middleware": "^0.1.2",
        "slim/csrf": "^0.8.1",
        "driftyco/ionicons": "^2.0",
        "FortAwesome/Font-Awesome": "*",
        "igorescobar/jquery-mask-plugin": "^1.14",
        "slim/flash": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "models/",
            "Components\\": "components/"
        }
    },
	"scripts": {
        "start": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
	}

}
